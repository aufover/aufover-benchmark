<                 <fo:list-item-label end-indent="label-end()">
<                   <fo:block>•</fo:block>
<                 </fo:list-item-label>
<                 <fo:list-item-body start-indent="body-start()">
<                   <fo:block>
<              <fo:inline font-family="Courier">&amp;mdash;</fo:inline> -- long dash (--)
< 	  </fo:block>
<                 </fo:list-item-body>
<               </fo:list-item>
<                 <fo:list-item-label end-indent="label-end()">
<                   <fo:block>•</fo:block>
<                 </fo:list-item-label>
